; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Fishy fishy"
run/main_scene="res://Scene stuff/Main/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")

[audio]

buses/default_bus_layout="res://Sound stuff/default_bus_layout.tres"

[autoload]

Memory="*res://code stuff/Managers/memory.gd"
MapMemory="*res://code stuff/Managers/map_memory.gd"
GameState="*res://code stuff/Managers/game_state.gd"
ApiManager="*res://code stuff/Managers/api_manager.gd"
PromptManager="*res://code stuff/Managers/prompt_manager.gd"
AudioManager="*res://code stuff/Managers/audio_manager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=3
window/size/borderless=true
window/stretch/mode="viewport"

[rendering]

textures/canvas_textures/default_texture_filter=0
2d/snap/snap_2d_vertices_to_pixel=true
